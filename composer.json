{
  "name": "quantic/atom",
  "type": "library",
  "description": "Quantic Framework atom for console and shell commands",
  "keywords": ["quantic","atom"],
  "homepage": "https://github.com/Quantical-Solutions/package-quantic-atom",
  "license": "MIT",
  "authors": [
    {
      "name": "Frédéric Geffray",
      "email": "in-touch-fg@quanticalsolutions.com",
      "homepage": "https://quanticalsolutions.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=7.3.0",
    "ext-json": "*",
    "wujunze/php-cli-color": ">=2.4.0"
  },
  "autoload": {
    "psr-4": {
      "Quantic\\Atom\\": "src/"
